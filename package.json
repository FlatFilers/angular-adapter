{
  "name": "angular-adapter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "prebuild:prod": "sh ./cp-readme.sh && rm -rf ./dist",
    "build:prod": "ng build angular-adapter --prod",
    "format": "prettier --write \"projects/**/*.ts\"",
    "publish:pkg": "cd dist/angular-adapter && npm publish",
    "publish:pkg:beta": "cd dist/angular-adapter && npm publish --tag beta",
    "deploy": "cd dist/angular-adapter && npm publish",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.2.6",
    "@angular/common": "~14.2.6",
    "@angular/compiler": "~14.2.6",
    "@angular/core": "~14.2.6",
    "@angular/forms": "~14.2.6",
    "@angular/platform-browser": "~14.2.6",
    "@angular/platform-browser-dynamic": "~14.2.6",
    "@angular/router": "~14.2.6",
    "@flatfile/sdk": "^2.3.2",
    "rxjs": "~7.5.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.6",
    "@angular/cli": "~14.2.6",
    "@angular/compiler-cli": "~14.2.6",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "18.7.1",
    "codelyzer": "^6.0.0",
    "commitlint": "^11.0.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "jasmine-core": "^4.4.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^14.2.1",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "10.9.1",
    "tslint": "~6.1.0",
    "typescript": "4.8.4"
  }
}
